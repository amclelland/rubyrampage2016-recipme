<!DOCTYPE html>
<html>
  <head>
    <title>Rumble</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="wrapper-for-content-outside-of-footer">
      <%= render 'site/header' %>
      <%= yield %>
    </div>
    <%= render 'site/footer' %>
  </body>
</html>
